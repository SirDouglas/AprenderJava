public class Exercicio025 {

	public static void main(String[] args) {
		double numero1 = Integer.parseInt(JOptionPane.showInputDialog("Digite um numero"));
	double numero2 = Integer.parseInt(JOptionPane.showInputDialog("Digite um segundo numero"));
			System.out.println(Math.pow(numero1,numero2));
			System.out.println(Math.sqrt(numero1));
	}
}
